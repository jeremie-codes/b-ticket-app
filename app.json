{
  "expo": {
    "name": "B-TICKETS",
    "slug": "b-ticket-app",
    "owner": "mastagate",
    "version": "0.1.2",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3f3923"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "",
      "buildNumber": "4"
    },
    "android": {
      "versionCode": 3,
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mastagate.btickets"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "description": "Un groupe qui œuvre dans le Marketing digital et vente de billets",
    "extra": {
      "eas": {
        "projectId": "b0f8b8e1-88e9-4684-a377-bd58c997bb70"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application accède à vos photos pour vous permettre de changer votre photo de profile."
        }
      ]
    ]
  }
}
